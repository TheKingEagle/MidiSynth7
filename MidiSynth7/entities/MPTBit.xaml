<ItemsControl x:Class="MidiSynth7.entities.MPTBit"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MidiSynth7.entities"
             UseLayoutRounding="True" Foreground="#FF234367" FontFamily="Segoe UI Mono" FontSize="16" FontWeight="Bold" BorderBrush="#FF274678" BorderThickness="0,0,2,0" Padding="2,0">
    <VirtualizingStackPanel Orientation="Horizontal" VirtualizingPanel.IsContainerVirtualizable="True"  VirtualizingPanel.VirtualizationMode="Recycling">
        
        <TextBlock x:Name="Bl_Notation" Background="{x:Null}"  FontFamily="Segoe UI Mono" Padding="0,0,2,0" Text="..."></TextBlock>
        <TextBlock x:Name="Bl_Instrument" Background="{x:Null}" FontFamily="Segoe UI Mono" Padding="2,0" Text=".."></TextBlock>
        <TextBlock x:Name="Bl_Velocity" Background="{x:Null}" FontFamily="Segoe UI Mono" Padding="2,0" Text=" .."></TextBlock>
        <TextBlock x:Name="Bl_ParmMark" Background="{x:Null}" FontFamily="Segoe UI Mono" Padding="2,0,0,0" Text="."></TextBlock>
        <TextBlock x:Name="Bl_ParmValue" Background="{x:Null}" FontFamily="Segoe UI Mono" Padding="0,0,2,0" Text=".."></TextBlock>
    </VirtualizingStackPanel>
</ItemsControl>
