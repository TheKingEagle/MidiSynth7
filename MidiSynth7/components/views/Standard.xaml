                    <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                        <GradientStop Color="#7F0C0C0C"/>
                        <GradientStop Color="#FF303030" Offset="1"/>
                    </LinearGradientBrush>
