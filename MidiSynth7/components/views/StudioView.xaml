                    <CheckBox.Effect>
                        <BlurEffect/>
                    </CheckBox.Effect>
                </CheckBox>
                <Button x:Name="MIO_bn_SetSF2" Content="Select Soundfont..." HorizontalAlignment="Left" Margin="4,3,0,0" VerticalAlignment="Top" Width="112" Height="29" Background="#FF38002E" BorderBrush="Red" Click="MIO_bn_SetSF2_Click" Focusable="False" IsEnabled="False" Grid.Column="1">
