                            <GradientStop Color="#FFB6AEAE"/>
                            <GradientStop Color="#FF272727" Offset="0.505"/>
                            <GradientStop Color="#FF3C3C3C" Offset="0.5"/>
                        </LinearGradientBrush>
                    </Ellipse.Stroke>
                </Ellipse>
