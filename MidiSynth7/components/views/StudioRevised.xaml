                            <SkewTransform/>
                            <RotateTransform/>
                            <TranslateTransform/>
                        </TransformGroup>
                    </SynthGUI:DialControl.RenderTransform>
                </SynthGUI:DialControl>
                <SynthGUI:DialControl x:Name="CTRL_Modulation" HorizontalAlignment="Left" Margin="28,30,0,0" VerticalAlignment="Top" Text="Modulation" Value="0" Maximum="127" ValueChanged="CTRL_ValueChanged" Width="125" RenderTransformOrigin="0.5,0.5" Grid.Column="11" Height="125" TextPromptStateChanged="Dials_TextPromptStateChanged" Cursor="Hand">
